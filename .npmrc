# Configuración de npm para DiagnoVET

# Configuración de registro
registry=https://registry.npmjs.org/

# Configuración de cache
cache-min=3600

# Configuración de auditoría
audit-level=moderate

# Configuración de fund
fund=false

# Configuración de save
save-exact=false

# Configuración para resolver conflictos de dependencias
legacy-peer-deps=true
force=true

# Configuración de engine-strict
engine-strict=true

# Configuración de prefer-offline
prefer-offline=true

# Configuración de progress
progress=true

# Configuración de color
color=true

# Configuración de loglevel
loglevel=warn
