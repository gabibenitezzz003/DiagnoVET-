{
  "name": "diagnovet-sistema-reportes",
  "version": "1.0.0",
  "description": "Sistema de procesamiento de reportes veterinarios con IA",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backend": "cd backend && python -m uvicorn main:app --reload --port 8000",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "deploy:staging": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh staging",
    "health-check": "chmod +x scripts/health-check.sh && ./scripts/health-check.sh",
    "backup": "chmod +x scripts/backup.sh && ./scripts/backup.sh",
    "docker:build": "docker-compose -f docker-compose.production.yml build",
    "docker:up": "docker-compose -f docker-compose.production.yml up -d",
    "docker:down": "docker-compose -f docker-compose.production.yml down",
    "docker:logs": "docker-compose -f docker-compose.production.yml logs -f",
    "docker:restart": "docker-compose -f docker-compose.production.yml restart",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "build:analyze": "ANALYZE=true npm run build",
    "precommit": "npm run lint && npm run type-check && npm run test",
    "predeploy": "npm run build && npm run test",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "install:backend:dev": "cd backend && pip install -r requirements-dev.txt",
    "install:all": "npm install && npm run install:backend",
    "install:all:dev": "npm install && npm run install:backend:dev"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@heroicons/react": "^2.2.0",
    "@n8n/chat": "^0.50.0",
    "@supabase/supabase-js": "^2.38.4",
    "@types/node": "20.8.0",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "autoprefixer": "10.4.16",
    "axios": "1.5.0",
    "canvas": "^3.2.0",
    "compression": "^1.8.1",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.2",
    "express-rate-limit": "^8.1.0",
    "framer-motion": "10.16.4",
    "franc": "^6.2.0",
    "fs-extra": "^11.3.2",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "lucide-react": "^0.292.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "next": "14.0.0",
    "node-cache": "^5.1.2",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.1",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^3.11.174",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-audio-recorder": "^2.2.0",
    "react-audio-voice-recorder": "^2.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "2.4.1",
    "react-markdown": "9.0.1",
    "react-pdf": "7.5.1",
    "react-syntax-highlighter": "15.6.6",
    "remark-gfm": "4.0.1",
    "sharp": "^0.34.4",
    "stream-json": "^1.9.1",
    "swagger-jsdoc": "^6.2.8",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "xregexp": "^5.1.2"
  },
  "devDependencies": {
    "@types/ajv": "^0.0.5",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express-rate-limit": "^5.1.3",
    "@types/franc": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/helmet": "^0.0.48",
    "@types/joi": "^17.2.2",
    "@types/moment": "^2.11.29",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/natural": "^5.1.5",
    "@types/node-cache": "^4.1.3",
    "@types/react-pdf": "^5.1.0",
    "@types/stream-json": "^1.7.8",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@types/xregexp": "^4.3.0",
    "concurrently": "8.2.2",
    "eslint": "8.51.0",
    "eslint-config-next": "14.0.0"
  }
}
